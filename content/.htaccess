<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{HTTP_HOST} theorymode.com$
RewriteRule ^(.*)$ "http://cmichel.io/$1" [R=301,L]
</IfModule>

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteCond %{HTTP_HOST}  ^www.cmichel.io [NC]
RewriteRule ^(.*) http://cmichel.io/$1 [L,R=301]
</IfModule>

<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /
RewriteRule ^index\.html$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule . /index.html [L]
</IfModule>


